<h1>Clinics</h1>

<% @clinics.each do |clinic| %>
  What: <%= clinic.what %><br>
  Where: <%= clinic.where %><br>
  When: <%= clinic.when.strftime("%m/%d/%y %H%p") %><br>
  How: <%= clinic.how %><br>
  <%= link_to 'Edit', edit_clinic_path(clinic) %>
  <%= link_to 'Destroy', clinic, method: :delete %>

  <br><br>
<% end %>

<%= link_to 'New Clinic', new_clinic_path %>