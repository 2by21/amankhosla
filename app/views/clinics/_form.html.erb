<%= form_for @clinic do |f| %>
  <%= f.label :what %></br>
  <%= f.text_field :what %></br>

  <%= f.label :where %></br>
  <%= f.text_field :where %></br>

  <%= f.label :how %></br>
  <%= f.text_field :how %></br>

  <%= f.label :when %></br>
  <%= f.datetime_select :when %></br>

  </br>

  <%= f.submit %>
<% end %>